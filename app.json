{
  "expo": {
    "name": "Tacto",
    "slug": "Tacto",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#364156"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.afan2.Tacto",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#364156"
      },
      "package": "com.afan2.Tacto",
      "googleServicesFile": "./secrets/google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Satoshi-Black.otf",
            "./assets/fonts/Satoshi-BlackItalic.otf",
            "./assets/fonts/Satoshi-Bold.otf",
            "./assets/fonts/Satoshi-BoldItalic.otf",
            "./assets/fonts/Satoshi-Italic.otf",
            "./assets/fonts/Satoshi-Light.otf",
            "./assets/fonts/Satoshi-LightItalic.otf",
            "./assets/fonts/Satoshi-Medium.otf",
            "./assets/fonts/Satoshi-MediumItalic.otf",
            "./assets/fonts/Satoshi-Regular.otf"
          ]
        }
      ],
      "expo-secure-store",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.785186330408-8nuhe77j47id1rkifod19qdqh1fe7bq3"
        }
      ],
      "expo-apple-authentication",
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "3e99ca79-2fa0-4db3-93a0-fd2b5b92bcf5"
      }
    }
  }
}
